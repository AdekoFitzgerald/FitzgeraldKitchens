(setq doMWMachine1HOLECam nil)
(defun doMWMachine1HOLECam (currentEnt entLayerName tempCAMStyle currentPanel file / mHoleX mHoleY mHoleZ mHoleDia mHoleDepth tempHoleDepth tempHolePos
																				  opOpName opSuitableTool opFeedRate tempHolePosWorld ikiyuzon sideOrigin
																				  tempPanelDepth tempPanelLength tempPanelWidth tempHolePosSide)
	(setq tempHolePos (getEntPro currentEnt 10))
	(setq ikiyuzon (getEntPro tempEnt 210))
	(setq mHoleDia (lmm_getLMMdata currentEnt lmm-k-diameter))
	(setq tempHoleDepth (lmm_getLMMdata currentEnt lmm-k-depth))
	;(setq opLayerName (nth 0 currentOp))
	(setq opSuitableTool (nth 1 tempCAMStyle));default tool numbers suitable for this macro
	(setq opOpName (nth 2 tempCAMStyle))
	(setq opFeedRate (nth 3 tempCAMStyle))
	(cond 
		((equal "TOP" (substr entLayerName 1 3))
			(setq mHoleX (lmm_convertNumtoStringWithDigit (abs (getnth 0 tempHolePos)) 2))
			(setq mHoleY (lmm_convertNumtoStringWithDigit (abs(getnth 1 tempHolePos)) 2))
			(setq mHoleZ (lmm_convertNumtoStringWithDigit (abs(getnth 2 tempHolePos)) 2))
			(setq mHoleDepth (lmm_convertNumtoStringWithDigit (abs tempHoleDepth) 2))
			(princ (sprintf "\nG100 X~s Y~s Z~s ~s ~s" (list mHoleX mHoleY mHoleDepth opFeedRate opSuitableTool)) file)
		)
		((equal "LEF" (substr entLayerName 1 3))
			;G182  HORIZONTAL HOLES IN WORKPIECE LEFT FACE
			;		X Hole co-ordinate
			;		Y        " 
			;		Z        " 
			;		K Offset on axis X
			;		F Boring speed
			;		T Tool number
			(setq tempPanelWidth (getnth 3 (getBBox currentPanel)))
			(setq tempHolePos (trans tempHolePos ikiyuzon 0))
			(setq mHoleX (lmm_convertNumtoStringWithDigit (abs (getnth 0 tempHolePos)) 2))
			(setq mHoleY (lmm_convertNumtoStringWithDigit (abs (- (abs tempPanelWidth) (abs (getnth 1 tempHolePos))) ) 2))
			(setq mHoleZ (lmm_convertNumtoStringWithDigit (abs (getnth 2 tempHolePos)) 2))
			(setq mHoleDepth (lmm_convertNumtoStringWithDigit (abs tempHoleDepth) 2))
			(princ (sprintf "\nG182 X~s Y~s Z~s ~s ~s" (list mHoleDepth mHoleY mHoleZ opFeedRate opSuitableTool)) file)
		)
		((equal "RIG" (substr entLayerName 1 3))
			;G183  HORIZONTAL HOLES IN WORKPIECE RIGHT FACE
			;      X Hole co-ordinate
			;      Y        " 
			;      Z        " 
			;      K Offset on axis X
			;      F Boring speed
			;      T Tool number
			(setq tempPanelWidth (getnth 3 (getBBox currentPanel)))
			(setq tempHolePos (trans tempHolePos ikiyuzon 0))
			(setq mHoleY (lmm_convertNumtoStringWithDigit (abs (- (abs tempPanelWidth) (abs (getnth 1 tempHolePos))) ) 2))
			(setq mHoleZ (lmm_convertNumtoStringWithDigit (abs(getnth 2 tempHolePos)) 2))
			(setq mHoleDepth (lmm_convertNumtoStringWithDigit (abs tempHoleDepth) 2))
			(princ (sprintf "\nG183 X~s Y~s Z~s ~s ~s" (list mHoleDepth mHoleY mHoleZ opFeedRate opSuitableTool)) file)
		)
		((equal "REA" (substr entLayerName 1 3))
			;G184  HORIZONTAL HOLES IN WORKPIECE REAR FACE
			;      X Hole co-ordinate
			;      Y        " 
			;      Z        " 
			;      K Offset on axis Y
			;      F Boring speed
			;      T Tool number
			(setq tempHolePos (trans tempHolePos ikiyuzon 0))
			(setq mHoleX (lmm_convertNumtoStringWithDigit (abs (getnth 0 tempHolePos)) 2))
			(setq mHoleZ (lmm_convertNumtoStringWithDigit (abs(getnth 2 tempHolePos)) 2))
			(setq mHoleDepth (lmm_convertNumtoStringWithDigit (abs tempHoleDepth) 2))
			(princ (sprintf "\nG184 X~s Y~s Z~s ~s ~s" (list mHoleX mHoleDepth mHoleZ opFeedRate opSuitableTool)) file)
		)
		((equal "FRO" (substr entLayerName 1 3))
			;G185  HORIZONTAL HOLES IN WORKPIECE FRONT FACE
			;      X Hole co-ordinate
			;      Y        " 
			;      Z        " 
			;      K Offset on axis Y
			;      F Boring speed
			;      T Tool number
			(setq tempHolePos (trans tempHolePos ikiyuzon 0))
			(setq mHoleX (lmm_convertNumtoStringWithDigit (abs (getnth 0 tempHolePos)) 2))
			(setq mHoleZ (lmm_convertNumtoStringWithDigit (abs(getnth 2 tempHolePos)) 2))
			(setq mHoleDepth (lmm_convertNumtoStringWithDigit (abs tempHoleDepth) 2))
			(princ (sprintf "\nG185 X~s Y~s Z~s ~s ~s" (list mHoleX mHoleDepth mHoleZ opFeedRate opSuitableTool)) file)
		)
		;; Masterwood can not do bottom drill
	)
)

(setq doMWMachine1SAWAlongX nil)
(defun doMWMachine1SAWAlongX (currentEnt entLayerName tempCAMStyle currentPanel file / tempEndPoint startPoint endPoint tempPoints 
								tempDepth stepOfPasses bottomZComponent topZComponent currentZLevel stepZComponets tempBBox currentPanelMiddlePoint tempPanelWidth
								toolCorrectionVector polylineIsStraitAlongByXAxis)
	(setq tempPoints (lmm_getPolylinePoints currentEnt))
	(setq polylineIsStraitAlongByXAxis T)
	(setq startPoint (nth 0 tempPoints))
	
	;Confirm polyline is along by x axis or not
	(foreach tempPoint tempPoints ;SAW blade can cut only x direction. so we must check whether given polyline is suitable to cut or not.
		(if (not (equal (nth 1 startPoint) (nth 1 tempPoint) 6))
			(progn
				(setq polylineIsStraitAlongByXAxis nil)
			)
		)
	)
	
	(if polylineIsStraitAlongByXAxis
		(progn
			(setq tempBBox (getBBox currentPanel))
			(setq tempPanelWidth (getnth 3 tempBBox))
			(setq currentPanelMiddlePoint (VectorScale (VectorAdd (nth 0 tempBBox) (nth 1 tempBBox)) 0.5))
			(setq endPoint (last tempPoints))
			(if (> (nth 0 startPoint) (nth 0 endpoint))
				(progn
					(setq tempEndPoint endPoint)
					(setq endPoint startPoint)
					(setq startPoint tempEndPoint)
				)
			)
			(setq tempDepth (lmm_getLMMdata currentEnt lmm-k-depth))
			(setq stepOfPasses 	(nth 3 tempCAMStyle))
			(setq bottomZComponent (+ tempDepth (nth 4 tempCAMStyle) ) )
			(setq topZComponent (- 0.0 (nth 5 tempCAMStyle) ) )
			(setq stepZComponets (_calculateZlevels stepOfPasses topZComponent bottomZComponent (nth 6 tempCAMStyle)))
			(cond 
				( (equal "CENTER" (nth 7 tempCAMStyle))
					(princ)
				)
				( (equal "INSIDE" (nth 7 tempCAMStyle))
					(setq toolCorrectionVector (list 0.0 
					(* (nth 8 tempCAMStyle) (/ (- (nth 1 currentPanelMiddlePoint) (nth 1 startPoint)) (abs (- (nth 1 currentPanelMiddlePoint) (nth 1 startPoint)))))
													0.0
					))
					(setq startPoint (VectorAdd startPoint toolCorrectionVector ))
					(setq endPoint (VectorAdd endPoint toolCorrectionVector ))
				)
				( (equal "OUTSIDE" (nth 7 tempCAMStyle))
					(setq toolCorrectionVector (list 0.0 
					(* (nth 8 tempCAMStyle) (/ (- (nth 1 startPoint) (nth 1 currentPanelMiddlePoint)) (abs (- (nth 1 startPoint) (nth 1 currentPanelMiddlePoint)))))
													0.0
					))
					(setq startPoint (VectorAdd startPoint toolCorrectionVector ))
					(setq endPoint (VectorAdd endPoint toolCorrectionVector ))
				)
			)
			(princ (sprintf "\n#902=#911-(~s)" (list (lmm_convertNumtoStringWithDigit (nth 1 startPoint) 2))) file)
			(princ "\n( ---------- " file)
			(princ (sprintf "\nG172 X~s Y=#902 Z~s ~s ~s ~s" (list (lmm_convertNumtoStringWithDigit (nth 0 startPoint) 2) 
																	(lmm_convertNumtoStringWithDigit (nth 2 startPoint) 2)
																	(nth 9 tempCAMStyle)
																	(nth 10 tempCAMStyle)
																	(nth 1 tempCAMStyle)
															)
					) 
			file)
			(princ (sprintf "\nG180 X~s ~s ~s" (list (lmm_convertNumtoStringWithDigit (nth 0 endPoint) 2)
													(nth 10 tempCAMStyle)
													(nth 1 tempCAMStyle)
												)
					)
			file)
		)
	)
	(princ)
)

