;entities that are can be applied operation "mwNotFreeEntities"
(setq sendMWMachine1 nil)
(defun sendMWMachine1 (cPnl machineProcedures / mwMachineName prgFilesPath tempPanelName tempPanelDepth tempPanelLength
												tempPanelWidth prgFile mwEntitiKType tempDerivedLayerName  
												tempItem currentOp tempEnt
						)
	(setq machineName "mwMakinesi1")
	(setq prgFilesPath (strcat (lmm_prepareOutDirs) "\\"))
	(setq tempPanelName (lmm_getLMMdata cPnl lmm-k-customCode))
	(setq tempPanelDepth (lmm_getLMMdata cPnl lmm-k-depth))
	(setq tempPanelLength (getnth 2 (getBBox cPnl)))
	(setq tempPanelWidth (getnth 3 (getBBox cPnl)))
	(createDir (strcat prgFilesPath machineName "\\" ))
	(setq prgFile (open (strcat prgFilesPath machineName "\\" (lmm_getDXFFileNameByFileMode (list cPnl)) ".prg") "w"))
	(setq tempPanelLength (lmm_convertNumtoStringWithDigit tempPanelLength 2))
	(setq tempPanelWidth (lmm_convertNumtoStringWithDigit tempPanelWidth 2))
	(setq tempPanelDepth (lmm_convertNumtoStringWithDigit tempPanelDepth 2))
	(princ "%" prgFile)
	(princ "\n|1 Adeko-generated project" prgFile)
	(princ "\n|2 0" prgFile)
	(princ "\n|3 1" prgFile)
	(princ (sprintf "\n|4 ~s" (list tempPanelLength)) prgFile)
	(princ (sprintf "\n|5 ~s" (list tempPanelDepth)) prgFile)
	(princ (sprintf "\n|6 ~s" (list tempPanelWidth)) prgFile)
	(princ "\n|7 0 0 0 0" prgFile)
	(princ "\n|8 0" prgFile)
	(princ "\n|9 " prgFile)
	(princ "\n|10 " prgFile)
	(princ "\n|11 0" prgFile)
	(princ "\n|12 " prgFile)
	(princ "\n|13 " prgFile)
	(princ "\n|14 " prgFile)
	(princ "\n|44 0" prgFile)
	(princ "\n:1" prgFile)
	(princ "\nG120 K1" prgFile)
	(princ "\n#910=DX" prgFile)
	(princ "\n#911=DY" prgFile)
	(foreach currentOp machineProcedures
		(setq tempOpLayerName (nth 0 currentOp))
		;(setq tempOpToolName (nth 1 currentOp))
		(setq tempOpOpName (nth 2 currentOp))
		(setq tempPanelSS (lmm_getComponentWithChild cPnl "ALL"))
		(setq tempPanelSSList (convertSStolist tempPanelSS))
		(foreach tempEnt tempPanelSSList
			(setq mwEntitiKType (lmm_getLMMdata tempEnt lmm-k-type))
			(if (equal (lmm_getLMMdata tempEnt lmm-k-type) "HOLE")
				(progn 
					(setq tempDerivedLayerName (mwInterpretHoleLayerName tempEnt))
				)
				(progn 
					(setq tempDerivedLayerName (getentpro  tempEnt 8 ))
				)
			)
			
			;Ýþlem yapýlamayan layer ismindeki varlýklarý yakalamk için tüm layerlarý *AllEntities te topluyoruz
			(if (not (member tempDerivedLayerName mwIgnoreLayerNameList))
				(if  (not (member (strcat tempPanelName "->"  tempDerivedLayerName) mwAllEntities))
					(setq mwAllEntities (append mwAllEntities (list (strcat tempPanelName "->"  tempDerivedLayerName))))
				)
			)

			(if (equal tempDerivedLayerName tempOpLayerName) ;; cam yapýlmasý gereken etity
				(progn
					(cond 
						((equal tempOpOpName "HOLE")
							(doMWMachine1HOLECam tempEnt tempDerivedLayerName currentOp cPnl prgFile)
							
							;Ýþlem yapýlamayan layer ismindeki varlýklarý yakalamk için tüm iþlem yapýlan varlýklarý *NotFreeEntities de topluyoruz
							(setq mwNotFreeEntities (append mwNotFreeEntities (list (strcat tempPanelName "->" tempDerivedLayerName))))
						) 
						((equal tempOpOpName "SAWX") ;testere iþlemi
							(doMWMachine1SAWAlongX tempEnt tempDerivedLayerName currentOp cPnl prgFile)
							
							;Ýþlem yapýlamayan layer ismindeki varlýklarý yakalamk için tüm iþlem yapýlan varlýklarý *NotFreeEntities de topluyoruz
							(setq mwNotFreeEntities (append mwNotFreeEntities (list (strcat tempPanelName "->"  tempDerivedLayerName))))
						)
						((equal tempOpOpName "SAWY") ;testere iþlemi
							(doMWMachine1SAWAlongY tempEnt tempDerivedLayerName currentOp cPnl prgFile)
							
							;Ýþlem yapýlamayan layer ismindeki varlýklarý yakalamk için tüm iþlem yapýlan varlýklarý *NotFreeEntities de topluyoruz
							(setq mwNotFreeEntities (append mwNotFreeEntities (list (strcat tempPanelName "->"  tempDerivedLayerName))))
						)
						((equal tempOpOpName "CONTOUR") ;freze iþlemi
							(doMWMachine1CONTOURCam tempEnt tempDerivedLayerName currentOp cPnl prgFile)
							
							;Ýþlem yapýlamayan layer ismindeki varlýklarý yakalamk için tüm iþlem yapýlan varlýklarý *NotFreeEntities de topluyoruz
							(setq mwNotFreeEntities (append mwNotFreeEntities (list (strcat tempPanelName "->"  tempDerivedLayerName))))
						)
					)
				)
			)
		)
	)
	
	(princ "\nM124 K1 D1" prgFile)
	(princ "\n%" prgFile)
	(close prgFile)
)




