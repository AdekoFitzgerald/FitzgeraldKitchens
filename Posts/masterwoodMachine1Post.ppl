(setq doMWMachine1SAWCam nil)
(defun doMWMachine1SAWCam (currentEnt entLayerName tempCAMStyle currentPanel file / tempPoints startPoint
															tempPoint polylineIsStraitAlongByYAxis polylineIsStraitAlongByXAxis)
	(setq tempPoints (lmm_getPolylinePoints currentEnt))
	(setq startPoint (nth 0 tempPoints))
	(setq polylineIsStraitAlongByXAxis T)
	(setq polylineIsStraitAlongByYAxis T)
	
	(foreach tempPoint tempPoints ;SAW blade can cut only x direction. so we must check whether given polyline is suitable to cut or not.
		(if (not (equal (nth 1 startPoint) (nth 1 tempPoint) 6))
			(progn
				(setq polylineIsStraitAlongByXAxis nil)
			)
		)
	)
	
	(foreach tempPoint tempPoints ;SAW blade can cut only x direction. so we must check whether given polyline is suitable to cut or not.
		(if (not (equal (nth 0 startPoint) (nth 0 tempPoint) 6))
			(progn
				(setq polylineIsStraitAlongByYAxis nil)
			)
		)
	)
	
	(cond
		(polylineIsStraitAlongByXAxis
			(doMWMachine1SAWAlongX currentEnt entLayerName tempCAMStyle currentPanel file);; if machine doesnot support x axis saw operation please do command here
		)
		(polylineIsStraitAlongByYAxis 
			(doMWMachine1SAWAlongY currentEnt entLayerName tempCAMStyle currentPanel file);; if machine doesnot support y axis saw operation please do command here
		)
		( (and (not polylineIsStraitAlongByXAxis) (not polylineIsStraitAlongByYAxis))
			(princ "(---CANNOT CREATE SAW OPERATION" file)
		)
	)
	
)


;(list 	"GROOVE" 
;		"T41" 
;		"CONTOUR" 
;		1  ;stepOfPasses
;		0  ;bottomZComponent (+) distance from entity thickness 
;		0  ;topZComponent (+) distance from safety plane (0.0)
;		0  ;lastPass Val (+) 
;		"CENTER" ;tool correction G40 G41 G42 stuff
;		0        ;reserved 
;		"S18"     ;spindle 
;		"F10"	;cut feedrate	
;		"E20"	;plunge feedrate	
;)
(setq doMWMachine1CONTOURCam nil)
(defun doMWMachine1CONTOURCam (currentEnt entLayerName tempCAMStyle currentPanel file / 
								radiusCorrection tempPoints tempDepth stepOfPasses tempPanelDepth stepZComponets currentZLevel
								bottomZComponent topZComponent spindleSpeed cutFeedRate plungeFeedRate iteration tempPoint currentTool
							)
	(setq tempDepth (lmm_getLMMdata currentEnt lmm-k-depth))
	(setq stepOfPasses 	(nth 3 tempCAMStyle))
	(setq bottomZComponent (+ tempDepth (nth 4 tempCAMStyle) ) )
	(setq topZComponent (- 0.0 (nth 5 tempCAMStyle) ) )
	(setq stepZComponets (_calculateZlevels stepOfPasses topZComponent  bottomZComponent  (nth 6 tempCAMStyle)) )
	(setq spindleSpeed (nth 9 tempCAMStyle))
	(setq cutFeedRate (nth 10 tempCAMStyle))
	(setq plungeFeedRate (nth 11 tempCAMStyle))
	(setq currentTool (nth 1 tempCAMStyle))
	(cond
		((equal "CENTER" (nth 7 tempCAMStyle))
			(setq tempPoints (lmm_getPolylinePoints currentEnt))
			(setq radiusCorrection nil) ;DISABLES TOOL RADIUS CORRECTION
		)
		
		( (equal "INSIDE" (nth 7 tempCAMStyle))
			(setq tempPoints (lmm_getPolylinePoints currentEnt))
			(if (equal "CCW" (GetIrregularPointlistClockwiseOrder tempPoints  currentPanel))
				(setq radiusCorrection "G41");ENABLES TOOL RADIUS CORRECTION ON THE LEFT
				(setq radiusCorrection "G42");ENABLES TOOL RADIUS CORRECTION ON THE RIGHT
			)
		)
		
		( (equal "OUTSIDE" (nth 7 tempCAMStyle))
			(setq tempPoints (lmm_getPolylinePoints currentEnt))
			(if (equal "CCW" (GetIrregularPointlistClockwiseOrder tempPoints  currentPanel))
				(setq radiusCorrection "G42");ENABLES TOOL RADIUS CORRECTION ON THE RIGHT
				(setq radiusCorrection "G41");ENABLES TOOL RADIUS CORRECTION ON THE LEFT
			)
		)
	)
	
	(foreach currentZLevel stepZComponets
		(setq iteration 0)
		(foreach tempPoint tempPoints
			(if (equal 0 iteration)
				(progn
					(if radiusCorrection
						(princ (sprintf "\n~s" (list radiusCorrection)) file)
						(princ "\nG40" file)
					)
					(princ (sprintf "\n#902=#911-(~s)" (list (lmm_convertNumtoStringWithDigit (nth 1 tempPoint) 2)) ) file)
					(princ "\n( ---------- " file)
					(princ (sprintf "\nG172 X~s Y=#902 Z~s ~s ~s ~s" (list (lmm_convertNumtoStringWithDigit (nth 0 tempPoint) 2)
																		;(lmm_convertNumtoStringWithDigit (nth 1 tempPoint) 2)
																		(lmm_convertNumtoStringWithDigit currentZLevel 2)
											 							spindleSpeed
											 							plungeFeedRate
																		currentTool
					)) file)
				)
				(progn
					(princ (sprintf "\n#902=#911-(~s)" (lmm_convertNumtoStringWithDigit (nth 1 tempPoint) 2)) file)
					(princ (sprintf "\nG101 X~s Y=#902 Z~s ~s" (list (lmm_convertNumtoStringWithDigit (nth 0 tempPoint) 2)
																	 ;(lmm_convertNumtoStringWithDigit (nth 1 tempPoint) 2)
																	 (lmm_convertNumtoStringWithDigit currentZLevel 2)
																	 cutFeedRate
					)) file)
				)
			)
			(setq iteration (+ 1 iteration))
		)
	)
	(princ "\nG40" file)
)




