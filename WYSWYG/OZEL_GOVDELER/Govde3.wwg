	nil
	dAltKot
	0.0
	;(d2r 90.0)
	0.0
	(list 0.0 0.0 0.0)
	"ALT_KUTU"
	""
	nil
	"SHELF"
	(strcat kodBlock "_ALT_KUTU")
	"LACQUER"
	"noedgedrawing"
)




(setq kodBlock (nxtBnam (lmm_sfRead kod)) )
