WARDROBE_HINGE_BASE_HOLES_ORIENT_TYPE -> "Vertical or Horizontal ?"

;prefvars
(_SETA 'WARDROBE_HINGE_BASE_HOLES_ORIENT_TYPE "VERTICAL")
(cond
	((equal HINGE_BASE_HOLES_ORIENT_TYPE 0)
		(setq WARDROBE_HINGE_BASE_HOLES_ORIENT_TYPE "VERTICAL")
	)
	((equal HINGE_BASE_HOLES_ORIENT_TYPE 1)
		(setq WARDROBE_HINGE_BASE_HOLES_ORIENT_TYPE "HORIZONTAL")
	)
)

(if (not (equal VARS1.HINGE_DO_NOT_DRILL_PANEL_HOLES 1)) (_RUNHELPERRCP  "CONNECTOR\\HINGE_BASE" T "p2chelper"))
(_NONOTCH)
