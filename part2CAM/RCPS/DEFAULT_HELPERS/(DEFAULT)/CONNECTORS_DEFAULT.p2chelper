							(if isFirstConn (progn (_FSET (_ 'visibleSideDominantConn "RAFIX_OPERATION")) (_FSET (_ 'isFirstConn nil))))
						)
						((equal op "SCREW_OPERATION")
							(_FSET (_ 'SCREW_OPERATION T))
							(if isFirstConn (progn (_FSET (_ 'visibleSideDominantConn "SCREW_OPERATION")) (_FSET (_ 'isFirstConn nil))))
