							(if isFirstConn (progn (_FSET (_ 'visibleSideDominantConn "CABINEO_OPERATION")) (_FSET (_ 'isFirstConn nil))))
						)
						((equal op "LINCO_OPERATION")
							(_FSET (_ 'LINCO_OPERATION T))
							(if isFirstConn (progn (_FSET (_ 'visibleSideDominantConn "LINCO_OPERATION")) (_FSET (_ 'isFirstConn nil))))
						)
						((equal op "LINCOv2_OPERATION")
							(_FSET (_ 'LINCOv2_OPERATION T))
							(if isFirstConn (progn (_FSET (_ 'visibleSideDominantConn "LINCOv2_OPERATION")) (_FSET (_ 'isFirstConn nil))))
						)
						((equal op "QFIX_OPERATION")
							(_FSET (_ 'QFIX_OPERATION T))
							(if isFirstConn (progn (_FSET (_ 'visibleSideDominantConn "QFIX_OPERATION")) (_FSET (_ 'isFirstConn nil))))
						)
						((equal op "BLUMFIX_OPERATION")
							(_FSET (_ 'BLUMFIX_OPERATION T))
							(if isFirstConn (progn (_FSET (_ 'visibleSideDominantConn "BLUMFIX_OPERATION")) (_FSET (_ 'isFirstConn nil))))
						)
						((equal op "FIXT_OPERATION")
							(_FSET (_ 'FIXT_OPERATION T))
							(if isFirstConn (progn (_FSET (_ 'visibleSideDominantConn "FIXT_OPERATION")) (_FSET (_ 'isFirstConn nil))))
						)
						((equal op "FIXT_SCREW_OPERATION")
							(_FSET (_ 'FIXT_SCREW_OPERATION T))
							(if isFirstConn (progn (_FSET (_ 'visibleSideDominantConn "FIXT_SCREW_OPERATION")) (_FSET (_ 'isFirstConn nil))))
						)						
						((equal op "RINGO_OPERATION")
							(_FSET (_ 'RINGO_OPERATION T))
							(if isFirstConn (progn (_FSET (_ 'visibleSideDominantConn "RINGO_OPERATION")) (_FSET (_ 'isFirstConn nil))))
