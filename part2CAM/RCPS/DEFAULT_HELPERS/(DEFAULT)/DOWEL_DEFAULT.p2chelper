;prefvars
; General dowel parameters
(_SETA 'DOWEL_DIAMETER 8)
(_SETA 'DOWEL_DEPTH 18)
(_SETA 'DOWEL_DEPTH_FOR_SIDES 14)

(_SETA 'DOWEL_HOR_POS (/ __AD_PANELTHICK 2))
(_FSET (_ 'DOWEL_VER_POS (- __AD_PANELTHICK DOWEL_HOR_POS)))
(_FSET (_ 'DOWEL_HOR_POS (* DOWEL_HOR_POS -1)))

(_SETA 'DOWEL_CODE "DOWEL_STANDARD")
(_SETA 'DOWEL_UNIT "pc")

; All dowel parameters set to a list
(_FSET (_ 'DOWEL_PARAMETERS (_ DOWEL_DIAMETER DOWEL_DEPTH DOWEL_DEPTH_FOR_SIDES DOWEL_CODE DOWEL_UNIT)))
		
(_NONOTCH)
