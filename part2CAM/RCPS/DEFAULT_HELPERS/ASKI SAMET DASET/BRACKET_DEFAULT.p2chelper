;prefvars
(_RUNDEFAULTHELPERRCP "OPERATIONS_BASE_DEFAULT" nil "p2chelper")
(_RUNDEFAULTHELPERRCP "GROOVES_DEFAULT" nil "p2chelper")

;General parameters
(_SETA 'IS_BRACKET_AVAILABLE T)
(_SETA 'BRACKET_TYPE "INSIDE")	; INSIDE or BACKSIDE

;Inside bracket parameters
(_SETA 'PERFORM_INSIDE_BRACKET_BACK_PANEL_OPERATIONS T)
		
(_SETA 'INSIDE_BRACKET_HOLES_DIA GENERAL_MARKER_DIA)
(_SETA 'INSIDE_BRACKET_HOLES_DEPTH GENERAL_MARKER_DEP)

(_SETA 'INSIDE_BRACKET_HOLES_VER_DIST_FOR_TOP (+ 30 __AD_PANELTHICK ))
(_SETA 'INSIDE_BRACKET_HOLES_HOR_AXIS_LIST_FOR_TOP (_ (+ 21 GROOVE_WID GROOVE_DISTANCE) (+ 21 32 GROOVE_WID GROOVE_DISTANCE)))
(_SETA 'INSIDE_BRACKET_HOLES_VER_SHIFTING_VALUE 0) ;SHIFTS NOTCH TOO

(_SETA 'PERFORM_INSIDE_BRACKET_BOTTOM_HOLES_OPERATIONS nil)
(_SETA 'INSIDE_BRACKET_HOLES_VER_DIST_FOR_BOTTOM 51)
(_SETA 'INSIDE_BRACKET_HOLES_HOR_AXIS_LIST_FOR_BOTTOM (_ (+ 14 GROOVE_WID GROOVE_DISTANCE) (+ 14 32 GROOVE_WID GROOVE_DISTANCE)))

(_SETA 'FLIP_INSIDE_BRACKET_HOLES_HOR_TO_VER nil)

(_SETA 'INSIDE_BRACKET_NOTCH_SHARE_HEI (+ 42 GROOVE_DEPTH))
(_SETA 'INSIDE_BRACKET_NOTCH_SHARE_WID (+ 11 GROOVE_DEPTH ))

(_SETA 'INSIDE_BRACKET_CODE_LEFT "BRACKET_SAMET_DASET_LEFT")
(_SETA 'INSIDE_BRACKET_CODE_RIGHT "BRACKET_SAMET_DASET_RIGHT")
(_SETA 'INSIDE_BRACKET_UNIT "pc")

;Backside bracket parameters
(_SETA 'PERFORM_BACKSIDE_BRACKET_BACK_PANEL_OPERATIONS T)

(_SETA 'BACKSIDE_BRACKET_REQUIRED_GROOVE_DISTANCE 0)

(_SETA 'BACKSIDE_BRACKET_HOLES_DIA GENERAL_MARKER_DIA)
(_SETA 'BACKSIDE_BRACKET_HOLES_DEPTH GENERAL_MARKER_DEP)
(_SETA 'BACKSIDE_BRACKET_HOLES_VER_DIST (+ 8 __AD_PANELTHICK ))
(_SETA 'BACKSIDE_BRACKET_HOLES_HOR_DIST 24)
(_SETA 'BACKSIDE_BRACKET_HOLES_GROUP_DIST 32)

(_SETA 'BACKSIDE_BRACKET_BACK_PANEL_HOLES_DIA 10)
(_SETA 'BACKSIDE_BRACKET_BACK_PANEL_HOLES_VER_DIST_LIST (_ (+ 24 GROOVE_DEPTH)))
(_SETA 'BACKSIDE_BRACKET_BACK_PANEL_HOLES_HOR_DIST (+ 24 GROOVE_DEPTH))

(_SETA 'NUMBER_OF_BACKSIDE_BRACKET_HOLES 2)

(_SETA 'BACKSIDE_BRACKET_CODE_LEFT INSIDE_BRACKET_CODE_LEFT)
(_SETA 'BACKSIDE_BRACKET_CODE_RIGHT INSIDE_BRACKET_CODE_RIGHT)
(_SETA 'BACKSIDE_BRACKET_UNIT "pc")

		(_SETA 'PERFORM_BRACKET_SIDE_PANEL_OPERATIONS nil)
		(_SETA 'PERFORM_BRACKET_SIDE_PANEL_OPERATIONS_LAYER_NAME "CONTOUR")
		(_SETA 'BRACKET_SIDE_OPERATION_NOTCH_START_FROM_TOP 29.0)
		(_SETA 'BRACKET_SIDE_OPERATION_BULGE_POINT_DIFF 9.0)
		(_SETA 'BRACKET_SIDE_OPERATION_NOTCH_SHARE_HEI 80.0)
		(_SETA 'BRACKET_SIDE_OPERATION_NOTCH_SHARE_WID 9.0)
		(_SETA 'BRACKET_SIDE_OPERATION_BULGE (tan (/ DEG_90 4.0)))
		(_SETA 'BRACKET_SIDE_OPERATION_APPROACH 9.0)
; CONTINGENCIES
(if (null (member PERFORM_INSIDE_BRACKET_BACK_PANEL_OPERATIONS (_ nil T))) (_FSET (_ 'PERFORM_INSIDE_BRACKET_BACK_PANEL_OPERATIONS T)))
(if (null (member PERFORM_BACKSIDE_BRACKET_BACK_PANEL_OPERATIONS (_ nil T))) (_FSET (_ 'PERFORM_BACKSIDE_BRACKET_BACK_PANEL_OPERATIONS T)))

; Standard Values, PERFORM_BRACKET_HOLES T, BRACKET_HOLES_DIA 8, BRACKET_HOLES_DEPTH 14, BRACKET_HOLES_VER_DIST 30, BRACKET_HOLES_HOR_DIST 30, BRACKET_HOLES_GROUP_DIST 30
; Standard Values, BRACKET_NOTCH_SHARE_HEI 50, BRACKET_NOTCH_SHARE_WID 50, BRACKET_CODE BLUM_BRACKET, BRACKET_UNIT pc
(_NONOTCH)
