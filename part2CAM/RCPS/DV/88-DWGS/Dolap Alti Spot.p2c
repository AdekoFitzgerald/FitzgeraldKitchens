(_SETA 'POLYLINE_CIRCLE_LAYER_NAME "POLYLINE_CIRCLE")
(_SETA 'ASPIRATOR_HOLE_DIAMETER (* gm1_altspotdia 10))
(_SETA 'ASPIRATOR_HOLE_FRONT_OFFSET (- (+ __DEP) (* gm1_altspotofset 10)))
(_SETA 'ASPIRATOR_HOLE_LEFT_SIDE_OFFSET (/ __WID 2))
(_SETA 'ASPIRATOR_POLYLINE_WID (* gm1_altspotdia 10))
(_SETA 'ASPIRATOR_POLYLINE_HEI (* gm1_altspotdia 10))
(_SETA 'ASPIRATOR_POLYLINE_SHIFT_VALUE 0)
