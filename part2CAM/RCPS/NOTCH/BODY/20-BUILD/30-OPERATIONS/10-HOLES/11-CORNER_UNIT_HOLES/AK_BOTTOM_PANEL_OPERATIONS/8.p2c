								(_ 0 0 nil AK_NOTCHED_BACK_PANEL_LEFT_WID) 
								(_ FITTING_PARAMETERS nil))))
							(_HOLEGROUP operationName AK_NOTCHED_BACK_PANEL_LEFT_CODE notchedBackPanelLeftPanelPointListForBottomPanel (_ "Y+" nil) connectorParams)
						)
					)
					
					(_FSET (_ 'index (+ index 1)))
				)
			)
