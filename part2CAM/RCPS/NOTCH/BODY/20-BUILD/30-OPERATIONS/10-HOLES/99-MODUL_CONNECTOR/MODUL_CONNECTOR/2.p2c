						(progn 
							(_FSET (_ 'holeCodeBody (_& (_ "NOTCHED_RIGHT_PANEL_MODUL_CONNECTOR_HOLE_" (getnth hCounter holeCodeParam)))))
							(_HOLE holeCodeBody NOTCHED_RIGHT_PANEL_CODE (_ currentHolePOS MODULE_CONNNECTOR_HOLES_DIAMETER MODULE_CONNNECTOR_HOLES_DEPTH))
						)
					)
					(_FSET (_ 'hCounter (+ 1 hCounter)))
				)
			)
		)
	)
