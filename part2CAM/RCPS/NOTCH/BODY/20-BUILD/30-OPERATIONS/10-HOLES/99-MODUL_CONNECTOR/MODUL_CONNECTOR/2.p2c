(if (> __NOTCHDIM1 __AD_PANELTHICK)
	(progn 
		(_FSET (_ 'holeCodeParam (_ "FU" "BU" "BD" "FD")))
		; Left panel operations are performed in original recipe
		(if (and MODULE_CONNNECTOR_IS_AVAILABLE (_EXISTPANEL NOTCHED_RIGHT_PANEL_CODE) (if __VRSIDE MODULE_CONNNECTOR_VSIDE_OP_AVAILABLE T) )
			(progn 
